<!--index.wxml-->
<view class="line-top"></view>
<view class="page">
  <view class="card" wx:for="{{list}}" wx:key="index">
      <view class="card-user">{{item.name}}</view>
      <view class="card-content">{{item.content}}</view>
      <view class="card-time">{{item.created_at}}</view>
  </view>
</view>

